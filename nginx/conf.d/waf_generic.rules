#favicon.ico不记录日志
location = /favicon.ico {
	expires 99d;
	log_not_found off;
	access_log off;
}

#robot.txt不存在不记录日志
location = /robots.txt {
	allow all;
	log_not_found off;
	access_log on;
}

#阻止路径或文件
if ($uri ~* .*(viewsource.jsp)$) { set $rule_id rule_103; return 404; }
#阻止 /~ 这种路径，通常用于匹配用户起始目录
if ($uri ~* .*(/~).*) { set $rule_id rule_104; return 444; }

#限制HTTP请求方法 替代modsecurity_crs_30_http_policy id:960032
#Only allow these request methods Do not accept DELETE, PUT,SEARCH and other methods
if ($request_method !~ ^(GET|HEAD|POST|OPTIONS)$ ) { set $rule_id rule105; return 444; }

#阻止文件类型（扩展名、后缀）替代modsecurity_crs_30_http_policy id:960035
location ~* .*\.(bzr|cvs|git|svn)\/ { set $rule_id rule_106; return 403; }
location ~* \.(bak|backup|bzr|cfg|conf|cvs|doc|docx|DS_Store|ear|git|gitignore|hg|htaccess|htpasswd|ini|inc|jar|log|online|production|project|properties|pl|pm|py|pyc|pyo|sh|sql|svn|swp|war)$ {  
	set $rule_id rule_107;
	return 403;  
}  

#阻止常见windows文件 替代modsecurity_crs_30_http_policy id:960035
location ~* \.(ade|adp|app|asa|ascx|ashx|asmx|asp|aspx|axd|bas|bat|cdx|cer|chm|class|cmd|com|config|cpl|crt|cs|csproj|csh|csr|dat|dbf|dll|dos|exe|fxp|hlp|hta|htr|htw|ida|idc|idq|ins|isp|its|jse|key|ksh|licx|lnk|mad|maf|mag|mam|maq|mar|mas|mat|mau|mav|maw|mda|mdb|mde|mdt|mdw|mdz|msc|msh|msh1|msh1xml|msh2|msh2xml|mshxml|msi|msp|mst|old|ops|pass|pcd|pdb|pif|pol|prf|prg|printer|pst|pwd|resources|resx|reg|rem|scf|scr|sct|shb|shs|shtm|shtml|soap|stm|sys|url|vb|vbe|vbs|vbproj|vsdisco|webinfo|xsd|xsx|ws|wsc|wsf|wsh)$ {
	set $rule_id rule_108;
	return 403;
}

